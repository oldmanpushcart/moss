<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane
        xmlns="http://javafx.com/javafx/17.0.12"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="io.github.oldmanpushcart.moss.frontend.javafx.controller.SplashController">
    <VBox prefHeight="359.0" prefWidth="561.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <ImageView fitHeight="374.0" fitWidth="561.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../statics/image/moss-splash-logo.png"/>
            <viewport>
                <Rectangle2D height="500.0" minX="60.0" minY="270.0" width="900.0"/>
            </viewport>
            <VBox.margin>
                <Insets/>
            </VBox.margin>
        </ImageView>
        <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" progress="0.0"/>
        <Label fx:id="tipsLabel" maxWidth="1.7976931348623157E308">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
    </VBox>
    <effect>
        <DropShadow/>
    </effect>
</AnchorPane>
